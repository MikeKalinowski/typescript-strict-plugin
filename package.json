{
  "name": "typescript-strict-plugin",
  "version": "1.0.0-beta.1",
  "description": "https://github.com/Microsoft/TypeScript/wiki/Writing-a-Language-Service-Plugin",
  "main": "dist/index.js",
  "private": false,
  "scripts": {
    "test": "npm run e2e",
    "build": "tsc",
    "dev": "tsc --watch -p .",
    "e2e": "node e2e/run.js",
    "e2e:coverage": "c8 -o e2e_coverage -x e2e -r html -i \"src/**/*\" node e2e/run.js"
  },
  "keywords": [
    "TypeScript",
    "TypeScript plugin tutorial",
    "TypeScript Language Service",
    "tutorial",
    "example"
  ],
  "author": "Sebasti√°n Gurin",
  "license": "MIT",
  "dependencies": {
    "typescript": "4.1.3"
  },
  "devDependencies": {
    "@types/node": "^14.14.35",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "c8": "^7.6.0",
    "eslint": "^7.22.0",
    "glob": "^7.1.6",
    "husky": "4.3.8",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1"
  },
  "lint-staged": {
    "*.(js|ts)": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
